{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "baseUrl": "./",
    "paths": {
      "@modules/*": ["./src/modules/*"],
      "@common/*": ["src/common/*"],
      "@services/*": ["src/services/*"]
    }
  },
  "minify": false
}